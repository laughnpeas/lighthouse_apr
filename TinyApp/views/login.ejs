<html>
  <head>
    <title></title>
  </head>
<body>
  <form action="/login" method="POST">
    <fieldset>
      <legend>Log In</legend>
      <% if(err) {%>
      <label style="color:brown" for="err"><%= err.statusCode %> <%= err %></label><br>
      <% } %>
        <label for="username">User Name:</label> 
          <input type="text" name="username" id="username" value="" placeholder="Enter your user name.">
        <br>
        <label for="password">Password:</label>
        <input type="text" name="password" type="password" id="password" value="" placeholder="Enter your password.">
        <br>
        <button>Log In</button>  <a href="/">Cancel</a>
    </fieldset>
  </form>
</body>
</html>